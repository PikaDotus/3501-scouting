<div class="separate">
	<h3>Teleop scoring (other)</h3>

	<%= f.label :catch_made, "Catches made" %>
	<input type="button" value="–" field="match[catch_made]" class="qtyminus"/>
	<%= f.number_field :catch_made, step: 1, min: 0, max: 30 %>
	<input type="button" value="+" field="match[catch_made]" class="qtyplus"/>
	<br>
	<%= f.label :catch_missed, "Catches missed" %>
	<input type="button" value="–" field="match[catch_missed]" class="qtyminus"/>
	<%= f.number_field :catch_missed, step: 1, min: 0, max: 30 %>
	<input type="button" value="+" field="match[catch_missed]" class="qtyplus"/>
	<br>
	<%= f.label :truss_made, "Truss shots made" %>
	<input type="button" value="–" field="match[truss_made]" class="qtyminus"/>
	<%= f.number_field :truss_made, step: 1, min: 0, max: 30 %>
	<input type="button" value="+" field="match[truss_made]" class="qtyplus"/>
	<br>
	<%= f.label :truss_missed, "Truss shots missed" %>
	<input type="button" value="–" field="match[truss_missed]" class="qtyminus"/>
	<%= f.number_field :truss_missed, step: 1, min: 0, max: 30 %>
	<input type="button" value="+" field="match[truss_missed]" class="qtyplus"/>
</div>
